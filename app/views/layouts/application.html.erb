<!DOCTYPE html>
<html>
  <head>
    <title>RspecTutorial</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div class="container">
      <div class="row"></div>
        <% if flash[:notice].present? %>
          <%= flash[:notice] %>
        <% end %>

        <%= yield %>
      </div>
    </div>
  </body>
</html>
